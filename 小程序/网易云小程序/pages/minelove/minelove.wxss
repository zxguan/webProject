/* pages/playList/playList.wxss */
.songItem{
    width: 100%;
    height: 180rpx;
    /* background-color: #999; */
    border-bottom: 1px solid #eeeeee;
    display: flex;
    padding: 15rpx;
  }
  
  .songItem>image{
    width: 180rpx;
    height: 180rpx;

    border-radius: 16rpx;
  }
  .songItem>.ka{
    display: flex;
    justify-content: center;
    align-items: center;
    padding-left: 250rpx;
  }
  .text{

    padding: 50rpx;
    font-size: 28rpx;
    color: #333;
    display: flex;
    flex-direction: column;
  }

